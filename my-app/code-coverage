pipeline {
    agent any



tools {
    jdk 'java'
    maven 'Maven'
}
         


    
        stage('git'){
            steps {
                git branch: 'main', url: 'https://github.com/devopsclouds/java-jar.git'
            }
        }
        
        stage('maven'){
            steps {
                sh 'mvn -f my-app/pom.xml clean install'
            }
        }
        
         
        
        
    }
}
